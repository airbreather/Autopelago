<Application 
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:autopelago="clr-namespace:Autopelago"
    xmlns:materialThemes="clr-namespace:Material.Styles.Themes;assembly=Material.Styles"
    xmlns:materialIcons="clr-namespace:Material.Icons.Avalonia;assembly=Material.Icons.Avalonia"
    x:Class="Autopelago.App"
    RequestedThemeVariant="Default">
    <Application.DataTemplates>
        <autopelago:ViewLocator/>
    </Application.DataTemplates>

    <Application.Styles>
        <materialThemes:MaterialTheme BaseTheme="Dark" PrimaryColor="Purple" SecondaryColor="Lime" />
        <materialIcons:MaterialIconStyles />
    </Application.Styles>
</Application>
