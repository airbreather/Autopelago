{
  "name": "autopelago",
  "version": "0.0.0",
  "builders": "./builders.json",
  "browserslist": [
    "baseline widely available on 2025-10-20"
  ],
  "overrides": {
    "browserslist": "^4.27.0"
  },
  "scripts": {
    "ng": "ng",
    "prestart": "yq . AutopelagoDefinitions.yml --output-format json --indent 0 > src/app/data/baked.json",
    "prebuild": "yq . AutopelagoDefinitions.yml --output-format json --indent 0 > src/app/data/baked.json",
    "prewatch": "yq . AutopelagoDefinitions.yml --output-format json --indent 0 > src/app/data/baked.json",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular-architects/ngrx-toolkit": "^20.4.2",
    "@angular/animations": "^20.3.10",
    "@angular/common": "^20.3.10",
    "@angular/compiler": "^20.3.10",
    "@angular/core": "^20.3.10",
    "@angular/forms": "^20.3.10",
    "@angular/platform-browser": "^20.3.10",
    "@angular/router": "^20.3.10",
    "@angular/service-worker": "^20.3.10",
    "@bitarray/typedarray": "^1.1.3",
    "@ngrx/signals": "^20.1.0",
    "angular-split": "^20.0.0",
    "archipelago.js": "^2.0.4",
    "immutable": "^5.1.4",
    "ngx-toastr": "^19.1.0",
    "normalize.css": "^8.0.1",
    "pure-rand": "^7.0.1",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "yocto-queue": "^1.2.1"
  },
  "devDependencies": {
    "@angular-eslint/builder": "^20.5.1",
    "@angular/build": "^20.3.9",
    "@angular/cli": "^20.3.9",
    "@angular/compiler-cli": "^20.3.10",
    "@eslint/js": "^9.39.1",
    "@node-rs/xxhash": "^1.7.6",
    "@stylistic/eslint-plugin": "^5.5.0",
    "@types/bun": "^1.3.2",
    "@typescript/native-preview": "^7.0.0-dev.20251107.1",
    "angular-eslint": "20.5.1",
    "eslint": "^9.39.1",
    "pixi-filters": "^6.1.4",
    "pixi.js": "^8.14.0",
    "typescript-eslint": "^8.46.3",
    "yaml": "^2.8.1"
  }
}