    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/platform-browser": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@ngrx/signals": "^19.2.1",
    "normalize.css": "^8.0.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-eslint/builder": "20.1.1",
    "@angular/build": "^20.0.5",
    "@angular/cli": "^20.0.5",
    "@angular/compiler-cli": "^20.0.0",
    "@eslint/js": "^9.29.0",
    "angular-eslint": "20.1.1",
    "eslint": "^9.29.0",
    "typescript": "~5.8.2",
    "typescript-eslint": "8.34.1"
  },
  "overrides": {
