{
  "name": "autopelago",
  "version": "0.0.0",
  "builders": "./builders.json",
  "web-types": "./web-types.json",
  "browserslist": [
    "baseline widely available on 2025-10-20"
  ],
  "scripts": {
    "ng": "ng",
    "bake-data": "yq . AutopelagoDefinitions.yml --output-format json --indent 0 > src/app/data/baked.json",
    "postinstall": "$npm_execpath run bake-data",
    "prestart": "$npm_execpath run bake-data",
    "prebuild": "$npm_execpath run bake-data",
    "prewatch": "$npm_execpath run bake-data",
    "pretest": "$npm_execpath run bake-data",
    "start": "ng serve --no-hmr",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "test": "ng test",
    "stamp-version": "jq -R -s 'split(\"\n\") | { \"version\": .[0], \"commit\": .[1], \"timestamp\": .[2] }' <(echo $GITHUB_REF_NAME) <(echo $GITHUB_SHA) <(date --utc --rfc-3339=seconds) >src/version-info.json",
    "prerelease": "$npm_execpath run stamp-version",
    "release": "$npm_execpath run build"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@airbreather/archipelago.js": "2.0.5-airbreather",
    "@angular-architects/ngrx-toolkit": "^20.6.0",
    "@angular/animations": "^21.1.0",
    "@angular/cdk": "21.1.0",
    "@angular/common": "^21.1.0",
    "@angular/compiler": "^21.1.0",
    "@angular/core": "^21.1.0",
    "@angular/forms": "^21.1.0",
    "@angular/platform-browser": "^21.1.0",
    "@angular/router": "^21.1.0",
    "@angular/service-worker": "^21.1.0",
    "@bitarray/typedarray": "^1.1.3",
    "@ctrl/tinycolor": "^4.2.0",
    "@ngrx/signals": "^21.0.1",
    "angular-split": "^20.0.0",
    "immutable": "^5.1.4",
    "ngx-toastr": "^19.1.0",
    "normalize.css": "^8.0.1",
    "pure-rand": "^7.0.1",
    "tslib": "^2.8.1",
    "yocto-queue": "^1.2.2"
  },
  "devDependencies": {
    "@angular-eslint/builder": "^21.1.0",
    "@angular/build": "^21.1.0",
    "@angular/cli": "^21.1.0",
    "@angular/compiler-cli": "^21.1.0",
    "@eslint/js": "^9.39.2",
    "@stylistic/eslint-plugin": "^5.7.0",
    "@types/bun": "^1.3.6",
    "angular-eslint": "21.1.0",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0",
    "vitest": "^4.0.17"
  },
  "trustedDependencies": [
    "@parcel/watcher",
    "core-js",
    "esbuild",
    "lmdb",
    "msgpackr-extract"
  ]
}
